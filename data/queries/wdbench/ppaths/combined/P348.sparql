# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P625> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P131>* <http://www.wikidata.org/entity/Q1196> .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c71 .
	?c71 <http://www.wikidata.org/prop/direct/P279>* <http://www.wikidata.org/entity/Q719456> .
}
# nl
SELECT * WHERE {
	?x1 <coordinate location> ?x2 .
	?x1 <located in the administrative territorial entity>* <Mecklenburg-Western Pomerania> .
	?x1 <instance of> ?c71 .
	?c71 <subclass of>* <station> .
}
# opt_blaze
SELECT DISTINCT * WHERE {
	hint:Query hint:optimizer "None" .
	?c71 <http://www.wikidata.org/prop/direct/P279>* <http://www.wikidata.org/entity/Q719456> .
	hint:Prior hint:gearing "reverse" .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c71 .
	?x1 <http://www.wikidata.org/prop/direct/P131>* <http://www.wikidata.org/entity/Q1196> .
	hint:Prior hint:gearing "forward" .
	?x1 <http://www.wikidata.org/prop/direct/P625> ?x2 .
}
# opt_virt
DEFINE sql:select-option "order" SELECT DISTINCT * WHERE {
	?c71 <http://www.wikidata.org/prop/direct/P279> <http://www.wikidata.org/entity/Q719456> OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 2) .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c71 .
	?x1 <http://www.wikidata.org/prop/direct/P131> ?v595 OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 1) .
	FILTER (IRI(?v595) = <http://www.wikidata.org/entity/Q1196>) .
	?x1 <http://www.wikidata.org/prop/direct/P625> ?x2 .
}