	?x1 <http://www.wikidata.org/prop/direct/P131> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?x3 .
	?x4 <http://www.wikidata.org/prop/direct/P150> ?x2 .
	?x4 <http://www.wikidata.org/prop/direct/P131>* <http://www.wikidata.org/entity/Q1197> .
	?x3 <http://www.wikidata.org/prop/direct/P279>* <http://www.wikidata.org/entity/Q15324> .
}
# nl
SELECT * WHERE {
	?x1 <located in the administrative territorial entity> ?x2 .
	?x1 <instance of> ?x3 .
	?x4 <contains the administrative territorial entity> ?x2 .
	?x4 <located in the administrative territorial entity>* <Lower Saxony> .
	?x3 <subclass of>* <body of water> .
}
