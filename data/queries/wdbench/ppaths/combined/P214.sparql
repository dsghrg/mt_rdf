# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P3896> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P131>* <http://www.wikidata.org/entity/Q25> .
}
# nl
SELECT * WHERE {
	?x1 <geoshape> ?x2 .
	?x1 <located in the administrative territorial entity>* <Wales> .
}
# opt_blaze
SELECT DISTINCT * WHERE {
	hint:Query hint:optimizer "None" .
	?x1 <http://www.wikidata.org/prop/direct/P131>* <http://www.wikidata.org/entity/Q25> .
	hint:Prior hint:gearing "reverse" .
	?x1 <http://www.wikidata.org/prop/direct/P3896> ?x2 .
}
# opt_virt
DEFINE sql:select-option "order" SELECT DISTINCT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P131> <http://www.wikidata.org/entity/Q25> OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 2) .
	?x1 <http://www.wikidata.org/prop/direct/P3896> ?x2 .
}