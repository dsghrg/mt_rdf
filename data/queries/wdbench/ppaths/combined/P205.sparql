# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P105> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P171> ?x3 .
	?x1 <http://www.wikidata.org/prop/direct/P225> ?x4 .
	<http://www.wikidata.org/entity/Q185230> <http://www.wikidata.org/prop/direct/P171>* ?x1 .
}
# nl
SELECT * WHERE {
	?x1 <taxon rank> ?x2 .
	?x1 <parent taxon> ?x3 .
	?x1 <taxon name> ?x4 .
	<Pteropodidae> <parent taxon>* ?x1 .
}
# opt_blaze

# opt_virt
DEFINE sql:select-option "order" SELECT DISTINCT * WHERE {
	<http://www.wikidata.org/entity/Q185230> <http://www.wikidata.org/prop/direct/P171> ?x1 OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 1) .
	?x1 <http://www.wikidata.org/prop/direct/P105> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P225> ?x4 .
	?x1 <http://www.wikidata.org/prop/direct/P171> ?x3 .
}