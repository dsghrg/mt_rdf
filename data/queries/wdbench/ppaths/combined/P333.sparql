# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P625> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P131>* <http://www.wikidata.org/entity/Q64> .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c605 .
	?c605 <http://www.wikidata.org/prop/direct/P279>* <http://www.wikidata.org/entity/Q41176> .
}
# nl
SELECT * WHERE {
	?x1 <coordinate location> ?x2 .
	?x1 <located in the administrative territorial entity>* <Berlin> .
	?x1 <instance of> ?c605 .
	?c605 <subclass of>* <building> .
}
# opt_blaze
SELECT DISTINCT * WHERE {
	hint:Query hint:optimizer "None" .
	?x1 <http://www.wikidata.org/prop/direct/P131>* <http://www.wikidata.org/entity/Q64> .
	hint:Prior hint:gearing "reverse" .
	?x1 <http://www.wikidata.org/prop/direct/P625> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c605 .
	?c605 <http://www.wikidata.org/prop/direct/P279>* <http://www.wikidata.org/entity/Q41176> .
	hint:Prior hint:gearing "forward" .
}
# opt_virt
DEFINE sql:select-option "order" SELECT DISTINCT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P131> <http://www.wikidata.org/entity/Q64> OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 2) .
	?x1 <http://www.wikidata.org/prop/direct/P625> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c605 .
	?c605 <http://www.wikidata.org/prop/direct/P279> <http://www.wikidata.org/entity/Q41176> OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 1) .
}