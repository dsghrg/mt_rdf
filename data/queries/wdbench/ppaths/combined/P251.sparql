# original
SELECT * WHERE {
	<http://www.wikidata.org/entity/Q7446404> <http://www.wikidata.org/prop/direct/P106> ?x1 .
	?x1 <http://www.wikidata.org/prop/direct/P279>* ?c487 .
	?c487 <http://www.wikidata.org/prop/direct/P425> ?x2 .
}
# nl
