# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P17> <http://www.wikidata.org/entity/Q36> .
	?x2 <http://www.wikidata.org/prop/direct/P19> ?x1 .
	?x2 <http://www.wikidata.org/prop/direct/P106>* <http://www.wikidata.org/entity/Q82955> .
}
# nl
SELECT * WHERE {
	?x1 <country> <Poland> .
	?x2 <place of birth> ?x1 .
	?x2 <occupation>* <politician> .
}
