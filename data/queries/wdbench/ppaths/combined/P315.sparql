# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P17> ?x2 .
	?x2 <http://www.wikidata.org/prop/direct/P300> ?x3 .
	?x1 <http://www.wikidata.org/prop/direct/P31>* <http://www.wikidata.org/entity/Q3918> .
}
# nl
SELECT * WHERE {
	?x1 <country> ?x2 .
	?x2 <ISO 3166-2 code> ?x3 .
	?x1 <instance of>* <university> .
}
# opt_blaze
SELECT DISTINCT * WHERE {
	hint:Query hint:optimizer "None" .
	?x1 <http://www.wikidata.org/prop/direct/P31>* <http://www.wikidata.org/entity/Q3918> .
	hint:Prior hint:gearing "reverse" .
	?x1 <http://www.wikidata.org/prop/direct/P17> ?x2 .
	?x2 <http://www.wikidata.org/prop/direct/P300> ?x3 .
}
# opt_virt
DEFINE sql:select-option "order" SELECT DISTINCT * WHERE {
	?x2 <http://www.wikidata.org/prop/direct/P300> ?x3 .
	?x1 <http://www.wikidata.org/prop/direct/P17> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P31> <http://www.wikidata.org/entity/Q3918> OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 1) .
}