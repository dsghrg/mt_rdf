# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P300> ?x2 .
	<http://www.wikidata.org/entity/Q28733587> <http://www.wikidata.org/prop/direct/P131>* ?x1 .
}
# nl
SELECT * WHERE {
	?x1 <ISO 3166-2 code> ?x2 .
	<No label defined> <located in the administrative territorial entity>* ?x1 .
}
