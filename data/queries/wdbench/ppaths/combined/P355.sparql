# opt_virt
DEFINE sql:select-option "order" SELECT DISTINCT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P969> ?x5 .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c215 .
	?c215 <http://www.wikidata.org/prop/direct/P279> ?v549 OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 1) .
	FILTER (IRI(?v549) = <http://www.wikidata.org/entity/Q5>) .
	?x1 <http://www.wikidata.org/prop/direct/P21> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P27> ?x3 .
	?x1 <http://www.wikidata.org/prop/direct/P3430> ?x4 .
}