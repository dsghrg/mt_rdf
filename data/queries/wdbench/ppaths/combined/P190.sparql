# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P131> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P31> <http://www.wikidata.org/entity/Q39715> .
	?x2 <http://www.wikidata.org/prop/direct/P131>* <http://www.wikidata.org/entity/Q55> .
}
# nl
SELECT * WHERE {
	?x1 <located in the administrative territorial entity> ?x2 .
	?x1 <instance of> <lighthouse> .
	?x2 <located in the administrative territorial entity>* <Netherlands> .
}
# opt_blaze
SELECT DISTINCT * WHERE {
	hint:Query hint:optimizer "None" .
	?x1 <http://www.wikidata.org/prop/direct/P31> <http://www.wikidata.org/entity/Q39715> .
	?x1 <http://www.wikidata.org/prop/direct/P131> ?x2 .
	?x2 <http://www.wikidata.org/prop/direct/P131>* <http://www.wikidata.org/entity/Q55> .
	hint:Prior hint:gearing "forward" .
}
# opt_virt
DEFINE sql:select-option "order" SELECT DISTINCT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P31> <http://www.wikidata.org/entity/Q39715> .
	?x1 <http://www.wikidata.org/prop/direct/P131> ?x2 .
	?x2 <http://www.wikidata.org/prop/direct/P131> <http://www.wikidata.org/entity/Q55> OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 1) .
}