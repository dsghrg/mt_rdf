# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P4000> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P171>+ <http://www.wikidata.org/entity/Q756> .
}
# nl
SELECT * WHERE {
	?x1 <has fruit type> ?x2 .
	?x1 <parent taxon>+ <plant> .
}
# opt_blaze
SELECT DISTINCT * WHERE {
	hint:Query hint:optimizer "None" .
	?x1 <http://www.wikidata.org/prop/direct/P171>+ <http://www.wikidata.org/entity/Q756> .
	hint:Prior hint:gearing "reverse" .
	?x1 <http://www.wikidata.org/prop/direct/P4000> ?x2 .
}
# opt_virt
DEFINE sql:select-option "order" SELECT DISTINCT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P171> <http://www.wikidata.org/entity/Q756> OPTION(TRANSITIVE, t_distinct, t_min(1), t_direction 2) .
	?x1 <http://www.wikidata.org/prop/direct/P4000> ?x2 .
}