# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P31> <http://www.wikidata.org/entity/Q3231690> .
	?x1 <http://www.wikidata.org/prop/direct/P31>* <http://www.wikidata.org/entity/Q3231690> .
}
# nl
SELECT * WHERE {
	?x1 <instance of> <automobile model> .
	?x1 <instance of>* <automobile model> .
}
