# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P625> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c15 .
	?c15 <http://www.wikidata.org/prop/direct/P279>* <http://www.wikidata.org/entity/Q23442> .
	?x1 <http://www.wikidata.org/prop/direct/P131>+ <http://www.wikidata.org/entity/Q90> .
}
# nl
SELECT * WHERE {
	?x1 <coordinate location> ?x2 .
	?x1 <instance of> ?c15 .
	?c15 <subclass of>* <island> .
	?x1 <located in the administrative territorial entity>+ <Paris> .
}
