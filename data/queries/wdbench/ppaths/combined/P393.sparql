# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P31> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P131>+ <http://www.wikidata.org/entity/Q1449> .
	?x2 <http://www.wikidata.org/prop/direct/P279>+ <http://www.wikidata.org/entity/Q486972> .
}
# nl
SELECT * WHERE {
	?x1 <instance of> ?x2 .
	?x1 <located in the administrative territorial entity>+ <Genoa> .
	?x2 <subclass of>+ <human settlement> .
}
# opt_blaze
SELECT DISTINCT * WHERE {
	hint:Query hint:optimizer "None" .
	?x1 <http://www.wikidata.org/prop/direct/P131>+ <http://www.wikidata.org/entity/Q1449> .
	hint:Prior hint:gearing "reverse" .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?x2 .
	?x2 <http://www.wikidata.org/prop/direct/P279>+ <http://www.wikidata.org/entity/Q486972> .
	hint:Prior hint:gearing "forward" .
}
# opt_virt
DEFINE sql:select-option "order" SELECT DISTINCT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P131> <http://www.wikidata.org/entity/Q1449> OPTION(TRANSITIVE, t_distinct, t_min(1), t_direction 2) .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?x2 .
	?x2 <http://www.wikidata.org/prop/direct/P279> <http://www.wikidata.org/entity/Q486972> OPTION(TRANSITIVE, t_distinct, t_min(1), t_direction 1) .
}