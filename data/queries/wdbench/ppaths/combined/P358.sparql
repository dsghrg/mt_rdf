# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P1340> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c132 .
	?c132 <http://www.wikidata.org/prop/direct/P279>* <http://www.wikidata.org/entity/Q5> .
}
# nl
SELECT * WHERE {
	?x1 <eye color> ?x2 .
	?x1 <instance of> ?c132 .
	?c132 <subclass of>* <human> .
}
# opt_blaze
SELECT DISTINCT * WHERE {
	hint:Query hint:optimizer "None" .
	?x1 <http://www.wikidata.org/prop/direct/P1340> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c132 .
	?c132 <http://www.wikidata.org/prop/direct/P279>* <http://www.wikidata.org/entity/Q5> .
	hint:Prior hint:gearing "forward" .
}
# opt_virt
DEFINE sql:select-option "order" SELECT DISTINCT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P1340> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c132 .
	?c132 <http://www.wikidata.org/prop/direct/P279> <http://www.wikidata.org/entity/Q5> OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 1) .
}