# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P131> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c842 .
	?c842 <http://www.wikidata.org/prop/direct/P279>* <http://www.wikidata.org/entity/Q7075> .
	?x1 <http://www.wikidata.org/prop/direct/P131> ?c650 .
	?c650 <http://www.wikidata.org/prop/direct/P131>* <http://www.wikidata.org/entity/Q25> .
}
# nl
SELECT * WHERE {
	?x1 <located in the administrative territorial entity> ?x2 .
	?x1 <instance of> ?c842 .
	?c842 <subclass of>* <library> .
	?x1 <located in the administrative territorial entity> ?c650 .
	?c650 <located in the administrative territorial entity>* <Wales> .
}
# opt_blaze
SELECT DISTINCT * WHERE {
	hint:Query hint:optimizer "None" .
	?c842 <http://www.wikidata.org/prop/direct/P279>* <http://www.wikidata.org/entity/Q7075> .
	hint:Prior hint:gearing "reverse" .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c842 .
	?x1 <http://www.wikidata.org/prop/direct/P131> ?c650 .
	?x1 <http://www.wikidata.org/prop/direct/P131> ?x2 .
	?c650 <http://www.wikidata.org/prop/direct/P131>* <http://www.wikidata.org/entity/Q25> .
	hint:Prior hint:gearing "forward" .
}
# opt_virt
DEFINE sql:select-option "order" SELECT DISTINCT * WHERE {
	?c650 <http://www.wikidata.org/prop/direct/P131> <http://www.wikidata.org/entity/Q25> OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 2) .
	?x1 <http://www.wikidata.org/prop/direct/P131> ?c650 .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c842 .
	?c842 <http://www.wikidata.org/prop/direct/P279> ?v364 OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 1) .
	FILTER (IRI(?v364) = <http://www.wikidata.org/entity/Q7075>) .
	?x1 <http://www.wikidata.org/prop/direct/P131> ?x2 .
}