# original
SELECT * WHERE {
	?x1 <http://www.wikidata.org/prop/direct/P686> ?x2 .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c907 .
	?c907 <http://www.wikidata.org/prop/direct/P279>* <http://www.wikidata.org/entity/Q2996394> .
}
# nl
SELECT * WHERE {
	?x1 <Gene Ontology ID> ?x2 .
	?x1 <instance of> ?c907 .
	?c907 <subclass of>* <biological process> .
}
# opt_blaze

# opt_virt
DEFINE sql:select-option "order" SELECT DISTINCT * WHERE {
	?c907 <http://www.wikidata.org/prop/direct/P279> <http://www.wikidata.org/entity/Q2996394> OPTION(TRANSITIVE, t_distinct, t_min(0), t_direction 2) .
	?x1 <http://www.wikidata.org/prop/direct/P31> ?c907 .
	?x1 <http://www.wikidata.org/prop/direct/P686> ?x2 .
}